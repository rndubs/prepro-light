{
  "name": "prepro-light",
  "displayName": "Prepro Light - Mesh Preprocessing",
  "description": "Lightweight mesh preprocessing tool for visualizing finite element meshes, contact surfaces, and material assignments",
  "version": "0.1.0",
  "publisher": "prepro-light",
  "repository": {
    "type": "git",
    "url": "https://github.com/rndubs/prepro-light.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "mesh",
    "vtk",
    "fea",
    "finite element",
    "visualization",
    "3d",
    "preprocessing"
  ],
  "activationEvents": [
    "onCustomEditor:prepro-light.meshEditor"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "prepro-light.meshEditor",
        "displayName": "Mesh Viewer",
        "selector": [
          {
            "filenamePattern": "*.vtp"
          },
          {
            "filenamePattern": "*.vtu"
          },
          {
            "filenamePattern": "*.vti"
          },
          {
            "filenamePattern": "*.stl"
          },
          {
            "filenamePattern": "*.obj"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "prepro-light.openMesh",
        "title": "Open Mesh File",
        "category": "Prepro Light"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "compile": "webpack --mode development",
    "lint": "eslint src --ext ts",
    "test": "vscode-test --label integration",
    "test:performance": "vscode-test --label performance",
    "test:legacy": "node ./dist/test/runTest.js",
    "test:performance:legacy": "node ./dist/test/runPerformanceTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.3.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "glob": "^11.0.3",
    "mocha": "^11.7.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@kitware/vtk.js": "^30.0.0"
  }
}
